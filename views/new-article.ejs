<%- include("./partials/header") %>

<section id="new-article">
  <h1>ADD NEW ARTICLE</h1>
  <div class="rectangle"></div>
  <form action="/new" method="POST">
    <input type="text" name="title" placeholder="Title" />
    <input type="text" name="url" placeholder="Url Picture" />
    <input type="text" name="author" placeholder="Author" />
    <input type="text" name="author_bild" placeholder="Author Picture" />
    <input type="text" name="body" placeholder="Write your article here....." />
    <input type="submit" value="SEND ARTICLE" />
  </form>
</section>
